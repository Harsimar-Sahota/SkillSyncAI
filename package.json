{
  "name": "skillsyncai-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "AI Agent-based skill-building assistant (Monorepo)",
  "scripts": {
    "dev": "pnpm --parallel --filter \"./server\" --filter \"./client\" run dev",
    "build": "pnpm --filter \"./**\" run build",
    "install:root": "pnpm install --shamefully-hoist",
    "clean": "pnpm --recursive exec -- rm -rf node_modules dist .turbo *.tsbuildinfo",
    "clean:hard": "pnpm clean && rm -rf node_modules .pnpm-store"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Harsimar-Sahota/SkillSyncAI.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Harsimar-Sahota/SkillSyncAI/issues"
  },
  "homepage": "https://github.com/Harsimar-Sahota/SkillSyncAI#readme",
  "devDependencies": {
    "@types/node": "^22.14.0",
    "typescript": "^5.8.3",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2"
  }
}
